                android:weightSum="2">

                <ImageView
                    android:id="@+id/ig"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:src="@drawable/iibomma16_ibannerad1" />

                <ImageView
                    android:id="@+id/ih"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:src="@drawable/iibomma16_ibannerad2" />

            </LinearLayout>

        </LinearLayout>


    </ScrollView>


    <FrameLayout
        android:id="@+id/fl_b"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@drawable/iibomma16_iqb1"
        android:gravity="bottom"
        android:visibility="visible" />


</LinearLayout>